{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typeindividus list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for typeIndividu in typeIndividus %}
            <tr>
                <td><a href="{{ path('typeindividu_show', { 'id': typeIndividu.id }) }}">{{ typeIndividu.id }}</a></td>
                <td>{{ typeIndividu.nom }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('typeindividu_show', { 'id': typeIndividu.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('typeindividu_edit', { 'id': typeIndividu.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('typeindividu_new') }}">Create a new typeIndividu</a>
        </li>
    </ul>
{% endblock %}
